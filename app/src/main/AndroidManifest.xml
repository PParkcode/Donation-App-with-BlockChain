<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.testing1">
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:name="androidx.multidex.MultiDexApplication"


        android:theme="@style/Theme.Testing1">

        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.Register"></activity>
        <activity android:name=".view.MainNav"></activity>
        <activity android:name=".view.SearchActivity"></activity>
        <activity android:name=".view.MoreCampaignActivity"></activity>
        <activity android:name=".view.DetailCampaign"></activity>
        <activity android:name=".view.RegistCampaign"></activity>
        <activity android:name=".view.MyData"></activity>
        <activity android:name=".view.PayActivity"></activity>
        <activity android:name=".view.WithDrawForm"></activity>
        <activity android:name=".view.DonateActivity"></activity>
        <activity android:name=".view.ExchangeActivity"></activity>
        <activity android:name=".view.RequestListActivity"></activity>
        <activity android:name=".view.ExchangeActivity2"></activity>
        <activity android:name=".view.MyTransaction"></activity>
        <activity android:name=".view.DetailWithDraw"></activity>

    </application>

</manifest>